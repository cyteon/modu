import "http" as http;
import "json" as json;

let data = json.new();
data.set("name", "modu");

let result = http.get("https://httpbin.org/get");
print(result.status, " - ", result.status_text);

let result = http.post("https://httpbin.org/post", json.stringify(data));
print(result.status, " - ", result.status_text);

let result = http.put("https://httpbin.org/put", json.stringify(data));
print(result.status, " - ", result.status_text);

let result = http.patch("https://httpbin.org/patch", json.stringify(data));
print(result.status, " - ", result.status_text);

let result = http.delete("https://httpbin.org/delete");
print(result.status, " - ", result.status_text);

let result = http.get("https://httpbin.org/delete");
print(result.status, " - ", result.status_text);