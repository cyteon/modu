print(5 + 5); // 10
print(10 - 2); // 8
print(5 * 5); // 25
print(10 / 2); // 5
print(1 * 1 + 2 * 2); // 5
print(3 ** 2); // 9
print(10 % 3); // 1
print(10 % 5); // 0

import "math";

// mul, div and pow can also be used as a function in math
print(math.mul(2, 3)); // 6
print(math.div(10, 2)); // 5
print(math.pow(3, 2)); // 9

print(math.abs(-5)); // 5